{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "entryPoints": [
    "./react-solution/configs",
    "./react-solution/container",
    "./react-solution/data-params-state",
    "./react-solution/dump",
    "./react-solution/env",
    "./react-solution/events",
    "./react-solution/http-client",
    "./react-solution/i18n",
    "./react-solution/log",
    "./react-solution/modals",
    "./react-solution/render",
    "./react-solution/router",
    "./react-solution/ssr",
    "./react-solution/state",
    "./react-solution/token",
    "./react-solution/types",
    "./react-solution/utils",
    "./react-solution/waiting-store"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "sortEntryPoints": false,
  "cleanOutputDir": true,
  "entryPointStrategy": "resolve",
  "excludePrivate": false,
  "readme": "none",
  "projectDocuments": ["./README.md"],

  "packageOptions": {
    "includeVersion": false,
    "excludePrivate": false,
    "readme": "./react-solution/README.md",
    "sortEntryPoints": false,
    "sort": "source-order",
    "excludeExternals": true,
    "gitRevision": "master"
  },

  "out": "./dist/docs",
  "disableSources": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "hideGroupHeadings": true,
  "useCodeBlocks": true,
  "parametersFormat": "table",
  "classPropertiesFormat": "list",
  "interfacePropertiesFormat": "table",
  "enumMembersFormat": "table",
  "propertyMembersFormat": "table",
  "typeDeclarationFormat": "table",
  "typeDeclarationVisibility": "compact",
  "mergeReadme": false,

  "outputFileStrategy": "members",
  "membersWithOwnFile": ["Variable", "Function", "Class", "Interface", "TypeAlias"],
  "expandObjects": false,
  "expandParameters": false,
  "indexFormat": "table",
  "tableColumnSettings": { "hideSources": true },
  "sanitizeComments": true
}
